{
  "name": "coursera-clone",
  "version": "1.0.0",
  "description": "A Coursera-like learning platform with comprehensive features",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "lint": "eslint src/",
    "build": "echo 'Build process here'",
    "client-install": "cd client && npm install",
    "client-build": "cd client && npm run build",
    "dev-full": "concurrently \"npm run dev\" \"cd client && npm start\""
